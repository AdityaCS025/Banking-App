{
    "name": "vaultbank-backend",
    "version": "1.0.0",
    "description": "Production-ready digital banking backend for Indian users",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "vercel-build": "tsc",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "db:migrate": "node scripts/migrate.js",
        "db:seed": "psql -U postgres -d vaultbank -f database/seed.sql"
    },
    "keywords": [
        "banking",
        "fintech",
        "nodejs",
        "express",
        "postgresql",
        "typescript"
    ],
    "author": "VaultBank Team",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.2",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^6.10.1",
        "pg": "^8.11.3",
        "uuid": "^9.0.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.10.5",
        "@types/nodemailer": "^6.4.21",
        "@types/pg": "^8.10.9",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "prettier": "^3.1.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}